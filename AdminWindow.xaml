        <TextBox HorizontalAlignment="Left" Margin="347,206,0,0" Text="Adresa" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Pretrazi" Name="Prezime" Click="Prezime_Click" HorizontalAlignment="Left" Margin="261,229,0,0" VerticalAlignment="Top"/>








    </Grid>
