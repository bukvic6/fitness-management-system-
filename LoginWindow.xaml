<Window x:Class="SR22_2020_POP2021.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SR22_2020_POP2021"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <Label Content="LOGIN" FontSize="30" HorizontalAlignment="Center" Margin="344,40,353.6,326"  />
        <Label Name="lblJMBG" Content="JMBG:" HorizontalAlignment="Left" Height="31" Margin="225,124,0,0" VerticalAlignment="Top" Width="108"/>
        <TextBox Name="txtJMBG"  Height="23" Margin="333,128,0,0" TextWrapping="Wrap" Text="{Binding Path=JMBG}" VerticalAlignment="Top" Width="120"/>
        <Label Content="Lozinka:" HorizontalAlignment="Left" Margin="226,198,0,0" VerticalAlignment="Top"/>
        <TextBox Name="txtLozinka" Height="23" Margin="505,193,168.6,0" TextWrapping="Wrap" Text="{Binding Path=Lozinka}" VerticalAlignment="Top" Width="120"/>
        <Button Content="OK" Click="OK_Click" HorizontalAlignment="Left" Margin="299,303,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Odustani" Click="Odustani_Click" HorizontalAlignment="Left" Margin="413,304,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
